{
  "name": "nest-typeorm-starter",
  "version": "1.0.0",
  "author": "ArtemKurtiak",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">=18.17.1"
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=PROD node dist/main",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "typeorm": "cross-env NODE_ENV=TYPEORM ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "datasource": "yarn typeorm -d ./src/shared/database/dataSource.ts",
    "migration:generate": "yarn datasource migration:generate",
    "migration:create": "yarn typeorm migration:create",
    "migration:run": "yarn datasource migration:run",
    "migration:revert": "yarn datasource migration:revert"
  },
  "dependencies": {
    "@nestjs/common": "^10.2.6",
    "@nestjs/core": "^10.2.6",
    "@nestjs/platform-express": "^10.2.6",
    "@nestjs/swagger": "^7.1.12",
    "@nestjs/typeorm": "^10.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "dotenv-expand": "^10.0.0",
    "pg": "^8.11.3",
    "pluralize": "^8.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.17",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.1.18",
    "@nestjs/schematics": "^10.0.2",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.89",
    "@types/cookie-parser": "^1.4.4",
    "@types/express": "^4.17.18",
    "@types/node": "20.6.5",
    "@types/pluralize": "^0.0.31",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "eslint": "^8.50.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.3",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.2.2"
  }
}
